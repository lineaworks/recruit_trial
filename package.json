{
  "name": "recruit_trial",
  "version": "1.0.0",
  "description": "リネアワークス中途採用コーディングテスト",
  "main": "index.js",
  "scripts": {
    "postinstall": "patch-package",
    "_clean:preview": "rimraf \"_preview/\"",
    "_clean:dest": "rimraf \"_dest/public_html/\"",
    "_watch:webpack": "webpack --watch",
    "_watch:gulp": "gulp watch",
    "_build:dev": "cross-env NODE_ENV=development gulp build",
    "_build:prod": "cross-env NODE_ENV=production gulp build",
    "_monitor": "npm-run-all -p _watch:*",
    "start": "cross-env NODE_ENV=development npm-run-all -s _clean:preview _build:dev _monitor",
    "watch": "cross-env NODE_ENV=development npm-run-all -s _monitor",
    "build": "cross-env NODE_ENV=production npm-run-all -s _clean:dest _build:prod",
    "_lint-scripts:config": "eslint -c .eslintrc.js .eslintrc.js --fix --ignore-pattern '!.eslintrc.js'",
    "_lint-scripts:src": "eslint *.js -c .eslintrc.js --fix",
    "lint-scripts": "npm-run-all -s _lint-scripts:*",
    "_lint-styles:stylelint": "stylelint src/**/*.scss --config .stylelintrc.json --fix",
    "_lint-styles:prettier": "prettier-stylelint src/**/*.scss --quiet --write",
    "lint-styles": "npm-run-all -s _lint-styles:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lineaworks/recruit_trial.git"
  },
